{
	"buildCommand": "npm run build",
	"outputDirectory": "dist",
	"framework": "vite",
	"rewrites": [
		{
			"source": "/api/riotgames/europe/:path*",
			"destination": "/api/riotgames/europe/:path*"
		},
		{
			"source": "/api/riotgames/eun1/:path*",
			"destination": "/api/riotgames/eun1/:path*"
		},
		{
			"source": "/ddragon-cdn/:path*",
			"destination": "/api/ddragon-cdn/:path*"
		}
	]
}
